{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ],
    "trailing-comma": false,
    "interface-name": false,
    "arrow-parens": [true, "ban-single-arg-parens"],
    "no-inferrable-types": false,
    "no-trailing-whitespace": false
  }
}
